<p id="notice"><%= notice %></p>

<h1>Questonnaires</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @questonnaires.each do |questonnaire| %>
      <tr>
        <td><%= link_to 'Show', questonnaire %></td>
        <td><%= link_to 'Edit', edit_questonnaire_path(questonnaire) %></td>
        <td><%= link_to 'Destroy', questonnaire, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Questonnaire', new_questonnaire_path %>
