  <header class="navbar transparent navbar-inverse navbar-fixed-top">
    <div class="container">
      <%= link_to "UniDates ❤", root_path, class: "navbar-brand logo" %>
      <button class="navbar-toggle" data-toggle="collapse" data-target=".navHeaderCollapse">
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
      </button>

      <div class="collapse navbar-collapse navHeaderCollapse">
        <ul class="nav navbar-nav navbar-right">
          <li><%= link_to "Home", root_path %></li>
          <% if logged_in? %>
            <% if current_user.questionnaire.blank? %>
              <li><%= link_to "Preferences", new_questionnaire_path %></li>
            <%else  %>
              <li><%= link_to "Update Preferences", edit_questionnaire_path(current_user.questionnaire.ids) %></li>

            <%end%>
            <li><%= link_to "Matched Users", questionnaires_path %></li>
            <li><%= link_to "Profile", current_user %></li>
            <li><%= link_to "Update Profile", edit_user_path(current_user) %></li>
            <li><%= link_to "Log Out", logout_path, method: :delete %></li>
          <% else %>
            <li><%= link_to "Sign Up", signup_path %></li>
            <li><%= link_to "Log In", login_path %></li>
          <% end %>
        </ul>
      </div>
    </div>
  </header>