<% provide(:title, 'Questions') %>
<h3>Questions</h3>



    <div class="card-deck">
<% Questionnaire.find_each do |questionnaire| %>
        <div class="card">
            <h5><%= questionnaire.question.question %></h5>
            <p1><%= questionnaire.answer.answer1 %></p1>
            <p2><%= questionnaire.answer.answer2 %></p2>
        </div>

    </div>
<%end%>

