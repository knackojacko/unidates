<% get_all_users.each do |user| %>
    <p><%= user.name %></p>
    <p><%= user.email %></p>
    
    <% if current_user.is_admin? %>
        <%= link_to "Profile", user_path(user), class: "btn btn-success btn-sm" %>
        <%= link_to "Delete user", user_path(user), method: :delete, class: "btn btn-danger btn-sm" %>
    <% end %>
    
    </br>
<%end%>